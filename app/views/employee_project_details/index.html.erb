<p id="notice"><%= notice %></p>

<h1>Employee Project Details</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Project detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_project_details.each do |employee_project_detail| %>
      <tr>
        <td><%= employee_project_detail.employee.name %></td>
        <td><%= employee_project_detail.project_detail.name %></td>
        <td><%= link_to 'Show', employee_project_detail %></td>
        <td><%= link_to 'Edit', edit_employee_project_detail_path(employee_project_detail) %></td>
        <td><%= link_to 'Destroy', employee_project_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee Project Detail', new_employee_project_detail_path %>
